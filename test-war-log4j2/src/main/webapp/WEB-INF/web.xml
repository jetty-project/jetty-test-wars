<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee webapp_4_0.xsd"
  version="4.0">
  <display-name>logging-log4j-webapp</display-name>
  <servlet>
    <display-name>LoggingServlet</display-name>
    <servlet-name>LoggingServlet</servlet-name>
    <servlet-class>org.mortbay.jetty.tests.webapp.LoggingServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>LoggingConfigServlet</display-name>
    <servlet-name>LoggingConfigServlet</servlet-name>
    <servlet-class>org.mortbay.jetty.tests.webapp.LoggingConfigServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoggingServlet</servlet-name>
    <url-pattern>/logging</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoggingConfigServlet</servlet-name>
    <url-pattern>/logging-config</url-pattern>
  </servlet-mapping>
</web-app>